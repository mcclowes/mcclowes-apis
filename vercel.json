{
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/.*", "dest": "/index.js" }
  ],
  "crons": [
    {
      "path": "/todos/process/reprioritise",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/todos/process/stale",
      "schedule": "0 7 1 * *"
    }
  ]
}
